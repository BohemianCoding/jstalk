var filter = [JSTQuickCIFilter quickFilterWithKernel:
"kernel vec4 foo(vec4 colorA, vec4 colorB) { \n" + 
"    vec2 dest = destCoord();\n" + 
"    vec4 ret  = dest.x < 100.0 ? colorA : colorB;\n" + 
"    return premultiply(ret);\n" + 
"}"];

[filter addKernelArgument:[CIColor colorWithString:"0.0 0.0 1.0 1.0"]];
[filter addKernelArgument:[CIColor colorWithString:"0.0 1.0 1.0 0.5"]];

[filter viewImageInWindowNamed:"test" extent:NSMakeRect(0, 0, 400, 400)];

